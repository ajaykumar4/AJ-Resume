%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

#% if certificates %#
\section{Certificates}
#% for certificate in certificates[:4] %#
\textbullet{} \href{#{ certificate.url }#}{#{ certificate.name }#}
#% endfor %#
\sectionsep
#% endif %#

