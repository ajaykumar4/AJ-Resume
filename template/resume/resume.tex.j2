%
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\namesection{#{ basics.name.split(' ')[0] }#}{#{ basics.name.split(' ')[1] }#}
{ 
\href{https://#{ basics.url }#}{\faLink\ #{ basics.url }#} |
\href{mailto:#{ basics.email }#}{\faEnvelope\ #{ basics.email }#} | 
\faMobile\ +353 892235142
\linebreak
\faMapMarker\ {#{ basics.location }#}
}

