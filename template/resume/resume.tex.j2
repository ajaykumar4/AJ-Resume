%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

\begin{minipage}[t]{0.33\textwidth} 

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
%     EDUCATION
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

#% if educations %#
\section{Education} 
#% for education in educations[:2] %#
\subsection{#{ education.institution }#}
\descript{#{ education.studyType }# in #{ education.area }#}
\location{#{ education.startDate }# - #{ education.endDate }# | #{ education.location }#}
\sectionsep
#% endfor %#
#% endif %#

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
%     LINKS
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

#% if links %#
\section{Links} 
#% for key, link in links.items() %#
\highlighttext{\href{https://#{ link.url }#}{\#{ link.icon }# \ #{ link.name }#}}
#% if not loop.last %#
\newline
#% endif %#
#% endfor %#
\sectionsep
#% endif %#

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
%     SKILLS
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

#% if skills %#
\section{Skills}
#% for skill in skills %#
\subsection{#{ skill.name }#}
#% for keyword in skill.keywords %#
#% if keyword.name and keyword.keywords %#
\location{\highlighttext{#{ keyword.name }#:}}
#% for item in keyword.keywords %#
\textbullet{}#{ item }#
#% endfor %#
#% else %#
\textbullet{}#{ keyword }#
#% endif %#
#% endfor %#
\sectionsep
#% endfor %#
#% endif %#

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
